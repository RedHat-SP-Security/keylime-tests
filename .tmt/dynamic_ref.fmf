adjust:
 - when: enforce_keylime_tests_branch is defined
   ref: $@enforce_keylime_tests_branch
   continue: false
 - when: distro = centos-stream-9
   ref: rhel-9-main
   continue: false
 - when: distro = rhel-9.1
   ref: rhel-9.1.0
   continue: false
 - when: distro = rhel-9
   ref: rhel-9-main
   continue: false
 - when: distro = fedora
   ref: fedora-main
   continue: false
